{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "formgent/page-break",
  "title": "Page Break",
  "category": "formgent",
  "description": "Seamlessly guide users to the next step.",
  "textdomain": "formgent",
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "name": {
      "type": "string",
      "default": "button"
    },
    "next_button_text": {
      "type": "string",
      "default": "Next"
    },
    "back_button_text": {
      "type": "string",
      "default": "Back"
    },
    "step_text": {
      "type": "string",
      "default": "Page Break"
    },
    "is_preview": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "interactivity": true
  },
  "editorScript": "file:index.js",
  "style": "file:style-index.css",
  "editorStyle": "file:index.css",
  "render": "file:render.php"
}