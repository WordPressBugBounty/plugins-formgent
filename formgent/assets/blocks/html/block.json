{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "formgent/html",
  "title": "HTML",
  "category": "formgent",
  "description": "Add custom HTML content to your form.",
  "textdomain": "formgent",
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "name": {
      "type": "string",
      "default": "html"
    },
    "label": {
      "type": "string",
      "default": "HTML Block"
    },
    "label_alignment": {
      "type": "string",
      "default": "top"
    },
    "sub_label": {
      "type": "string",
      "default": ""
    },
    "html_content": {
      "type": "string",
      "default": ""
    },
    "block_width": {
      "type": "string",
      "default": "100"
    },
    "logics": {
      "type": "object",
      "default": {}
    },
    "is_preview": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "interactivity": true
  },
  "editorScript": "file:index.js",
  "style": "file:style-index.css",
  "editorStyle": "file:index.css",
  "render": "file:render.php"
}