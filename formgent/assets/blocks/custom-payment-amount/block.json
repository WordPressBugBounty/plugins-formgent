{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "formgent/custom-payment-amount",
  "title": "Custom Payment Amount",
  "category": "formgent-payment",
  "description": "Include this field to allow users to enter a custom payment amount.",
  "textdomain": "formgent",
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "name": {
      "type": "string",
      "default": "custom-payment-amount"
    },
    "value": {
      "type": "string",
      "default": ""
    },
    "placeholder": {
      "type": "string",
      "default": ""
    },
    "label": {
      "type": "string",
      "default": "Custom Payment Amount"
    },
    "label_alignment": {
      "type": "string",
      "default": "top"
    },
    "sub_label": {
      "type": "string",
      "default": ""
    },
    "required": {
      "type": "boolean",
      "default": false
    },
    "limit": {
      "type": "boolean",
      "default": false
    },
    "min": {
      "type": "number",
      "default": 1
    },
    "max": {
      "type": "number",
      "default": 100
    },
    "block_width": {
      "type": "string",
      "default": "100"
    },
    "logics": {
      "type": "object",
      "default": {}
    },
    "is_preview": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "interactivity": true
  },
  "editorScript": "file:index.js",
  "style": "file:style-index.css",
  "editorStyle": "file:index.css",
  "render": "file:render.php"
}