{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "formgent/step",
  "title": "Step",
  "category": "formgent",
  "description": "Break forms into clear, manageable stages in conversation form.",
  "textdomain": "formgent",
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "media": {
      "type": "object",
      "default": {}
    },
    "media_brightness": {
      "type": "string",
      "default": "0"
    },
    "brightness_color": {
      "type": "string",
      "default": "#000000"
    },
    "layout": {
      "type": "string",
      "default": "media_left"
    },
    "logics": {
      "type": "object",
      "default": {}
    }
  },
  "supports": {
    "interactivity": true,
    "inserter": false
  },
  "editorScript": "file:index.js",
  "style": "file:style-index.css",
  "editorStyle": "file:index.css",
  "render": "file:render.php"
}